    %pad/u 32;
    %cmpi/u 99, 0, 32;
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x5605e63b3b10_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5605e63b3b10_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5605e63b3b10_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5605e6362f20;
T_1 ;
    %wait E_0x5605e63a0ae0;
    %load/vec4 v0x5605e63b3cc0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5605e63b3c00_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5605e63b3b10_0;
    %load/vec4 v0x5605e6364350_0;
    %cmp/u;
    %jmp/0xz  T_1.2, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5605e63b3c00_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x5605e63b3b10_0;
    %pad/u 32;
    %cmpi/e 99, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5605e63b3c00_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5605e63b3c00_0, 0;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
